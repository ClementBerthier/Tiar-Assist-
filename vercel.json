{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "redirects": [
        {
            "source": "/:path*",
            "has": [{ "type": "host", "value": "tiareassist.fr" }],
            "destination": "https://www.tiareassist.fr/:path*",
            "permanent": true
        }
    ],
    "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }]
}
